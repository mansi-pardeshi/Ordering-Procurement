        <name>Contract Start Date Validation for renewal</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }

  var oldDate = g_form.getValue('contract_renewal_old_contract_end_date');
  // Convert the new date string to a Date object
  var newDate = new Date(newValue);
  var oldDateObj = new Date(oldDate);
 
  // Check if the new date is before today
  if (newDate < oldDateObj) {
    // Show an error message and clear the field value
    g_form.showFieldMsg('contract_renewal_select_new_contract_start_date', 'Please select a date onwards from OLD CONTRACT END DATE.', 'error');
  }

}]]></script>
