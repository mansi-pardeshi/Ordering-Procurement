        <name>Contract End Date for new</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }
   // Get today's date in milliseconds
  var today = new Date();
  var startDate = g_form.getValue('new_vendor_select_contract_start_date');
 
  // Convert the new date string to a Date object
  var newDate = new Date(newValue);
  var startDateObj = new Date(startDate)
 
  // Check if the new date is before today
  if ((newDate < today)||(newDate < startDateObj)) 
  {
    // Show an error message and clear the field value
    g_form.showFieldMsg('new_vendor_select_contract_end_date', 'Please select a date onwards of start date.', 'error');
  }
}]]></script>
